    "tsc": "tsc --project tsconfig.json --skipLibCheck --noEmit",
    "lint": "yarn eslint && yarn tsc",
    "clean": "rimraf dist tsc",
    "build": "yarn clean && yarn lint && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WojciechCendrzak/i18n-keys.git"
  },
  "prepublish": "tsc",
  "keywords": [
    "i18n",
    "i18next",
    "internationalization",
    "key"
