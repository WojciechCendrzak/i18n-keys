  "bugs": {
    "url": "https://github.com/WojciechCendrzak/i18n-keys/issues"
  },
  "homepage": "https://github.com/WojciechCendrzak/i18n-keys/blob/main/README.md",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "15.6.0",
    "coveralls": "3.1.0",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.6",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.4"
  }
